{% extends 'base.html.twig' %}


{# templates/cheval/nouveau.html.twig #}



{% block body %}
    <h1>Ajouter un cheval</h1>

    {{ form_start(form) }}
    {{ form_row(form.nom) }}
    {{ form_row(form.race) }}
    {{ form_row(form.sexe) }}
    {{ form_row(form.sire) }}
    {{ form_row(form.prixDeDepart) }}
    

    <button type="submit" class="btn btn-primary">Enregistrer</button>
    {{ form_end(form) }}
{% endblock %}
